{
    "$schema": "https://railway.app/railway.schema.json",
    "build": {
        "builder": "DOCKERFILE",
        "dockerfilePath": "Dockerfile"
    },
    "deploy": {
        "startCommand": "php-fpm -D && nginx -g 'daemon off;'",
        "restartPolicyType": "ON_FAILURE",
        "healthCheckPath": "/",
        "healthCheckTimeout": 100
    }
}